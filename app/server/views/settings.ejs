    <div id="page-wrapper">
        <div class="container">
            <div class="row">
                <div class="col-md-6 col-md-offset-3">
                    <div class="login-panel panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title"><strong>User Settings</strong></h3>
                        </div>
                        <div class="panel-body">
                            <form role="form" id="signupForm" action="/signup" method="post">
                                <fieldset form="signupForm">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="firstName">First Name <span class="required-asterisk">*</span></label>
                                                <input class="form-control" id="firstName" placeholder="John" name="firstName" type="text" autofocus required>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="lastName">Last Name <span class="required-asterisk">*</span></label>
                                                <input class="form-control" id="lastName" placeholder="Wick" name="lastName" type="text" required>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="username">Username <span class="required-asterisk">*</span></label>
                                        <input class="form-control" id="username" placeholder="boogieman123" name="username" type="text" required>
                                    </div>

                                    <div class="form-group">
                                        <label for="gender">Gender <span class="required-asterisk">*</span></label>
                                        <select class="form-control" id="gender" name="gender" required>
                                            <option disabled selected value> -- Select an option -- </option>
                                            <option value ="Male">Male</option>
                                            <option value="Female">Female</option>
                                        </select>
                                    </div>

                                    <div class="form-group">
                                        <label for="age">Age <span class="required-asterisk">*</span></label>
                                        <input class="form-control" id="age" placeholder="20" name="age" type="number" min="10" max="100" required>
                                    </div>

                                    <div class="form-group">
                                        <label for="javaProficiency">Proficiency in Java <span class="required-asterisk">*</span></label>
                                        <select class="form-control" id="javaProficiency" name="javaProficiency" required>
                                            <option disabled selected value> -- Select an option -- </option>
                                            <option value="Beginner">Beginner</option>
                                            <option value="Intermediate">Intermediate</option>
                                            <option value="Advanced">Advanced</option>
                                        </select>
                                    </div>

                                    <div class="form-group">
                                        <label for="knownTopics">Topics Known in Java <span class="required-asterisk">*</span></label>
                                        <select multiple class="form-control" id="knownTopics" name="knownTopics" required>
                                            <option value="Arrays">Arrays</option>
                                            <option value="2D_Arrays">2D Arrays</option>
                                            <option value="Arithmetics">Arithmetic</option>
                                            <option value="Classes">Classes</option>
                                            <option value="Constructor">Constructor</option>
                                            <option value="Control">Control</option>
                                            <option value="DataType">Data Types</option>
                                            <option value="Decisions">Decisions</option>
                                            <option value="Expression">Expressions</option>
                                            <option value="InputOutput">Input-Output</option>
                                            <option value="Java">Java Basics</option>
                                            <option value="Loops">Loops</option>
                                            <option value="Method">Methods</option>
                                            <option value="Objects">Objects</option>
                                            <option value="Operator">Operators</option>
                                            <option value="Primitive Data Type">Primitive Data Types</option>
                                            <option value="Strings">Strings</option>
                                            <option value="Variables">Variables</option>
                                        </select>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="academicExperience">Academic Experience <span class="required-asterisk">*</span></label>
                                                <select class="form-control" id="academicExperience" name="academicExperience" required>
                                                    <option disabled selected value> -- Select an option -- </option>
                                                    <option value="0 - 2 years">0 - 2 years</option>
                                                    <option value="2 - 6 years">2 - 6 years</option>
                                                    <option value="6+ years">6+ years</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="professionalExperience">Professional Experience <span class="required-asterisk">*</span></label>
                                                <select class="form-control" id="professionalExperience" name="professionalExperience" required>
                                                    <option disabled selected value> -- Select an option -- </option>
                                                    <option value="0 - 2 years">0 - 2 years</option>
                                                    <option value="2 - 6 years">2 - 6 years</option>
                                                    <option value="6+ years">6+ years</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="educationLevel">Highest Level of Education Completed <span class="required-asterisk">*</span></label>
                                        <select class="form-control" id="educationLevel" name="educationLevel" required>
                                            <option disabled selected value> -- Select an option -- </option>
                                            <option value="High School Graduate">High School Graduate</option>
                                            <option value="Associate Degree">Associate Degree</option>
                                            <option value="Bachelor's Degree">Bachelor's Degree</option>
                                            <option value="Master's Degree">Master's Degree</option>
                                        </select>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="universityName">Current University <span class="required-asterisk">*</span></label>
                                                <input class="form-control" id="universityName" placeholder="Arizona State University" name="universityName" type="text" required>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="universityLocation">University Location <span class="required-asterisk">*</span></label>
                                                <input class="form-control" id="universityLocation" placeholder="Tempe, AZ" name="universityLocation" type="text" required>
                                            </div>
                                        </div>
                                    </div>

                                </fieldset>
                            </form>
                        </div>
                        <div class="panel-heading">
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- jQuery -->
        <script src="js/vendor/jquery.min.js"></script>
        <!-- Bootstrap Core JavaScript -->
        <script src="js/vendor/bootstrap.min.js"></script>

        <script>
            var error = <%- JSON.stringify(error) %>;
            $("document").ready(function () {
               // $("#signupForm :input").attr("disabled", true);
                var username = <%- JSON.stringify(username) %>;
                var firstName = <%- JSON.stringify(firstName) %>;
                var lastName = <%- JSON.stringify(lastName)%>;
                var gender = <%- JSON.stringify(gender) %>;
                var age = <%- JSON.stringify(age)%>;
                var javaProficiency = <%- JSON.stringify(javaProficiency)%>;
                var academicExperience = <%- JSON.stringify(academicExperience)%>;
                var professionalExperience = <%- JSON.stringify(professionalExperience)%>;
                var educationLevel = <%- JSON.stringify(educationLevel)%>;
                var knownTopics= <%- JSON.stringify(knownTopics)%>;
                var universityName = <%- JSON.stringify(universityName) %>;
                var universityLocation = <%- JSON.stringify(universityLocation) %>;
                values = knownTopics.split(",");
                var knownSelector = document.getElementById("knownTopics");
                for (var j =0; j<knownSelector.options.length; j++) {
                    for (var i = 0; i < values.length; i++) {
                        if (knownSelector.options[j].value === values[i]) {
                            knownSelector.options[j].selected = true;
                        }
                    }
                }

                document.getElementById("gender").value=gender;
                document.getElementById("firstName").value=firstName;
                document.getElementById("lastName").value=lastName;
                document.getElementById("username").value=username;
                document.getElementById("age").value=age;
                document.getElementById("javaProficiency").value=javaProficiency;
                document.getElementById("academicExperience").value=academicExperience;
                document.getElementById("professionalExperience").value=professionalExperience;
                document.getElementById("educationLevel").value=educationLevel;
                document.getElementById("universityName").value=universityName;
                document.getElementById("universityLocation").value=universityLocation;
                $("#signupForm :input").attr("disabled", true);
            });

        </script>

    <!-- /#page-wrapper - WRITE CONTENT HERE -->
    </div>


</div>

</body>

