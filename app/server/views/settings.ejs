                            <div class="profile_info">
                                <h4 id="fullname" style="padding-left: 5px; padding-right: 5px; text-align: center;"></h4>
                            </div>
                            <script>
                                var fullname = "<%= firstName %>" + "<%= lastName %>";
                                var gender = "<%= gender %>";
                                if (gender === "Male") {
                                    document.getElementById("avatar").src = "images/male.png";
                                }
                                else {
